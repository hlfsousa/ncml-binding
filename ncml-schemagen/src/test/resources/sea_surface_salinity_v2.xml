<netcdf location="http://opendap.jpl.nasa.gov/opendap/allData/aquarius/L3/mapped/V2/7day/SCI/2013/134/Q20131342013140.L3m_7D_SCI_V2.0_SSS_1deg.bz2"  xmlns="http://www.unidata.ucar.edu/namespaces/netcdf/ncml-2.2">
  <dimension name="Latitude" length="180"/>       
  <dimension name="Longitude" length="360"/>       
  <variable name="l3m_data" shape="Latitude Longitude" orgName="_l3m_data" type="float">
    <attribute type="string" name="units" value="psu" />
    <attribute type="string" name="long_name" value="Sea Surface Salinity" />
  </variable>
  <variable name="Latitude" shape="Latitude" type="float">
    <values start="89.5" increment="-1"/>
    <attribute type="string"  name="units" value="degrees_north" />
  </variable>
  <variable name="Longitude" shape="Longitude" type="float">
    <values start="-179.5" increment="1"/>
    <attribute type="string"  name="units" value="degrees_east" />
  </variable>

  <group name="metadata">
    <dimension name="revision_count" isUnlimited="true" />
    <attribute name="authors" type="string" />
    <variable name="revision_date" type="long" shape="revision_count" />
    <attribute name="version" type="int" />
  </group>
  
</netcdf>
